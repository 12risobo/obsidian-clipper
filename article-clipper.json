{
	"schemaVersion": "0.1.0",
	"name": "Article",
	"behavior": "create",
	"noteContentFormat": "{{content}}",
	"properties": [
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "Categories",
			"value": "[[Articles]]",
			"type": "multitext"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings, To read",
			"type": "multitext"
		},
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "published",
			"value": "{{published}}",
			"type": "date"
		},
		{
			"name": "author",
			"value": "{{author|wikilink}}",
			"type": "multitext"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "image",
			"value": "{{image}}",
			"type": "text"
		},
		{
			"name": "words",
			"value": "{{words}}",
			"type": "number"
		}
	],
	"triggers": [],
	"noteNameFormat": " {{title}}",
	"path": "0_Inbox"
}