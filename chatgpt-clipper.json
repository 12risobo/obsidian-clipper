{
	"schemaVersion": "0.1.0",
	"name": "ChatGPT",
	"behavior": "create",
	"noteContentFormat": "## Highlights \n{{highlights|map:item => item.text (item.timestamp)|join:\"\\n\\n\"}}\n\n{{selectorHtml:article[data-testid*=\"conversation-turn\"]|replace:\"/<svg.*?svg>/g\":\"\"|join:\"\"|remove_html:(\".text-token-text-secondary,.text-token-text-tertiary\")|markdown|replace:(\"/\\s*###### ChatGPT said\\:\\s*/g\":\"\\n\\n[!ChatGPT-response]\\n\",\"/##### You said\\:\\s*/g\":\"[!ChatGPT-prompt]\\n\", \"/^(#{1,6} .*?) \\*\\*(.*)\\*\\*$/gm\":\"$1 $2\")|blockquote|replace:\"\\n> \\n> [!ChatGPT-\":\"\\n\\n> [!ChatGPT-\"}}",
	"properties": [
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "Categories",
			"value": "[[AI-Chat]]",
			"type": "multitext"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings",
			"type": "multitext"
		},
		{
			"name": "topic",
			"value": "{{\\\"topics in this article, separated by commas\\\"}}",
			"type": "multitext"
		},
		{
			"name": "aliases",
			"value": "",
			"type": "multitext"
		},
		{
			"name": "Related notes",
			"value": "",
			"type": "multitext"
		}
	],
	"triggers": [
		"https://chatgpt.com/share",
		"https://chatgpt.com/c"
	],
	"noteNameFormat": "{{selector:title|safe_name}} - ChatGPT",
	"path": "0_Inbox"
}