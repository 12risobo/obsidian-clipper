{
	"schemaVersion": "0.1.0",
	"name": "The Hacker News",
	"behavior": "create",
	"noteContentFormat": "{{content}}",
	"properties": [
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "Categories",
			"value": "[[Articles]]",
			"type": "multitext"
		},
		{
			"name": "Type",
			"value": "Ycombinator",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings",
			"type": "multitext"
		},
		{
			"name": "status",
			"value": "To read",
			"type": "text"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{author}}",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{published}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://thehackernews.com/",
		"https://news.ycombinator.com/"
	],
	"noteNameFormat": "{{selectorHtml:.post-body > meta[itemprop=\"datePublished\"]?content|date:\"YYYY-MM-DD\"}} {{title|safe_name}} - Ycombinator",
	"path": "0_Inbox"
}